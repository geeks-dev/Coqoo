    "keywords": ["cms"],
    "homepage": "http://coqoo.net",
    "license": "MIT",
    "authors": [
        {
            "name": "Gilbert Pellegrom",
            "email": "gilbert@pellegrom.me"
        }
    ],
    "require": {
        "php": ">=5.3",
        "twig/twig": "1.12.*",
        "michelf/php-markdown": "1.3",
        "geeks-dev/php-markdown-extra-extended-stylish": "dev-master",
        "monolog/monolog": ">=1.0.0"
    },
    "autoload": {
    }
}